{
  "name": "trello_up",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint"
  },
  "dependencies": {
    "axios": "^0.19.2",
    "bootstrap": "^4.5.0",
    "bootstrap-select": "^1.13.17",
    "core-js": "^3.6.5",
    "firebase": "^7.15.3",
    "jquery": "^3.5.1",
    "lodash": "^4.17.15",
    "node-sass": "^4.14.1",
    "popper.js": "^1.16.1",
    "qs": "^6.9.4",
    "sass-loader": "^8.0.2",
    "v-clipboard": "^2.2.3",
    "vue": "^2.6.11",
    "vue-i18n": "^8.18.2",
    "vue-router": "^3.3.4",
    "vuelidate": "^0.7.5",
    "vuex": "^3.4.0",
    "wow.js": "^1.2.2"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^4.4.4",
    "@vue/cli-plugin-eslint": "^4.4.4",
    "@vue/cli-service": "^4.4.4",
    "@vue/eslint-config-airbnb": "^5.0.2",
    "babel-eslint": "^10.1.0",
    "eslint": "7.2.0",
    "eslint-config-airbnb-base": "^14.2.0",
    "eslint-plugin-import": "^2.21.2",
    "eslint-plugin-vue": "^6.2.2",
    "vue-template-compiler": "^2.6.11"
  },
  "eslintConfig": {
    "root": true,
    "extends": [
      "airbnb-base",
      "eslint:recommended",
      "plugin:vue/base"
    ],
    "plugins": [
      "vue"
    ],
    "rules": {
      "no-console": "off",
      "arrow-parens": [
        "error",
        "as-needed"
      ],
      "comma-dangle": [
        "error",
        {
          "arrays": "never",
          "objects": "never",
          "imports": "never",
          "exports": "never",
          "functions": "never"
        }
      ],
      "import/extensions": [
        "error",
        "ignorePackages",
        {
          "vue": "always"
        }
      ]
    },
    "env": {
      "es6": true,
      "node": true,
      "jquery": true
    },
    "parserOptions": {
      "parser": "babel-eslint"
    }
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions"
  ]
}
