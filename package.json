{
  "name": "trello_form",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint"
  },
  "dependencies": {
    "axios": "^0.19.2",
    "bootstrap": "^4.4.1",
    "bootstrap-select": "^1.13.14",
    "core-js": "^3.6.5",
    "firebase": "^7.14.1",
    "jquery": "^3.5.0",
    "lodash": "^4.17.15",
    "node-sass": "^4.13.1",
    "popper.js": "^1.16.1",
    "prettier": "^2.0.4",
    "sass-loader": "^8.0.2",
    "vue": "^2.6.11",
    "vue-i18n": "^8.17.3",
    "vue-router": "^3.1.6",
    "vuelidate": "^0.7.5",
    "vuex": "^3.2.0",
    "wow.js": "^1.2.2"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^4.3.1",
    "@vue/cli-plugin-eslint": "^4.3.1",
    "@vue/cli-service": "^4.3.1",
    "@vue/eslint-config-airbnb": "^5.0.2",
    "babel-eslint": "^10.1.0",
    "eslint": "^6.8.0",
    "eslint-config-airbnb-base": "^14.1.0",
    "eslint-config-prettier": "^6.10.1",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-prettier": "^3.1.3",
    "eslint-plugin-vue": "^6.2.2",
    "vue-template-compiler": "^2.6.11"
  },
  "eslintConfig": {
    "root": true,
    "extends": [
      "airbnb-base",
      "eslint:recommended",
      "prettier",
      "plugin:vue/base"
    ],
    "plugins": [
      "prettier",
      "vue"
    ],
    "rules": {
      "no-console": "off",
      "prettier/prettier": "error",
      "import/extensions": [
        "error",
        "ignorePackages",
        {
          "vue": "always"
        }
      ]
    },
    "env": {
      "es6": true,
      "node": true,
      "jquery": true
    },
    "parserOptions": {
      "parser": "babel-eslint"
    }
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions"
  ]
}
